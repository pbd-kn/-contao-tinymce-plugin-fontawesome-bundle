{
  "name": "pbd-kn/contao-tinymce-plugin-fontawesome-bundle",
  "type": "contao-bundle",
  "description":"Add a fontawesome to tinyMCE rte.",
  "keywords":["contao", "plugin", "fontawesome", "tinyMCE"],
  "license": "LGPL-3.0+",
  "authors": [
        {
            "name": "pbd-kn",
            "email": "pb-contao@gmx.de",
            "role": "Developer"
        }
  ],
  "require": {
    "php": "^8",
    "contao/core-bundle": "^4.4.2",
    "markocupic/contao-tinymce-plugin-builder-bundle": "*@dev"
  },
  "repositories": [
    {
      "type": "path",
      "url": "C:\\wampneu\\www\\co4\\Github Bundles\\contao-tinymce-plugin-builder-bundle"
    }
  ],

  "autoload": {
    "psr-4": {
      "Pbdkn\\ContaoTinymcePluginFontawesomeBundle\\": "src/"
    },    
    "classmap": [
      "src/Resources/contao"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/languages/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Pbdkn\\ContaoTinymcePluginFontawesomeBundle\\ContaoManager\\Plugin"
  }
}

